    .tipoprecioanterior
      margin-right 40px
      margin-left 100px
    border-bottom 1px solid #CCC
  .detalles
    .nombre-atributo
      font-size: 90%
      margin-right 8px
  #header-information
    z-index 100
    button, input
      font-size 160%
  button.change-attr-button
    font-size 120%
  button.add-observation-button
    font-size 100%
    width 65px
  .my-grid tr td.botonera-precios
    white-space nowrap
  .my-grid tr td[my-colname=precio], .my-grid tr td[my-colname=precioanterior]
    font-size 120%
  .my-grid tr td[my-colname=tipoprecio], .my-grid tr td[my-colname=tipoprecioanterior]
    font-size 120%
  tfoot
    display none
  .head-info-row
    display none
  .my-grid[my-table^=mobile_at]
    td
      padding 16px
    td[my-colname=copiar_atributo]
      text-align center
  .bp-input
    font-size 100%
    margin 0px
    padding 0px

