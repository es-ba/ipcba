#cache-layout, #cache-log
  clear both
  position relative
  z-index 9999999

#cache-status
  padding 0px 10px
  margin 0px

app-cache-version, #app-dm-version
  padding 0px 8px

#app-versions
  padding 8px 0px
  background-color transparent
  color #f4f4f4
  letter-spacing 1px
  font-weight bold
  position absolute
  right 175px
  

.mobile-main-layout #header-information
    background-color #d4cdc8
    border-bottom 1px solid #a9aeb9
    border-left 1px solid #a9aeb9
    border-right 1px solid #a9aeb9
    color #35353d
    font-family Helvetica
    font-weight bold
    font-size 75%
    padding 5px 0px
    margin: 0px 0px 5px

.mobile-main-layout #header-information[set-fixed=true]
  position fixed
  width 651px
  margin-left 92px
  top 0px
    
.mobile-main-layout #header-information[set-fixed=true] .back-button
  display none

.mobile-main-layout #header-information[set-fixed=true] button
  padding-top 2px
  padding-bottom 2px
  font-size 100%

.mobile-main-layout #header-information #search-product
    float right
    padding 0 3px 0 0

.mobile-main-layout #header-information #search-product input
    padding 3px 6px
    width 200px

#header-information #panel, #header-information #tarea, #header-information #encuestador
    padding 5px 8px

.mobile-main-layout .grid-mobile .my-grid
    table-layout fixed
    
.mobile-main-layout .grid-mobile .my-grid .grid-th-actions, .mobile-main-layout .grid-mobile .my-grid .head-info-empty-col, .mobile-main-layout .grid-mobile .my-grid .foot-info-empty-col, .mobile-main-layout .grid-mobile .my-grid .foot-info-insert-col
    display none

.mobile-main-layout .grid-mobile .my-grid .grid-caption
    display none

.mobile-main-layout #title
    font-family Helvetica
    font-weight bold
    font-size 75%
    letter-spacing 1px
    border-bottom 1px solid #a9aeb9
    border-top 1px solid #a9aeb9
    border-left 1px solid #a9aeb9
    border-right 0px solid #a9aeb9
    margin 0px
    border-spacing 0px
    border-collapse collapse
    border-right 1px solid #a9aeb9
    background #456b71
    padding 6px
    color #ececec

button[delete=true]
    background-color #cb3837
    color #ffffff

[is-collapsed=false] [my-collapsable=true]
    display block

[is-collapsed=false] .extra-row [my-collapsable=true]
    display table-cell

[is-collapsed=true] [my-collapsable=true]
    display none

td[selected-from-collapsed=true]
    background-color:  #ebe1ca !important

#formularios-sidebar
    float left
    width 92px

#formularios-sidebar .formulario
    padding 5px
    margin 5px 5px 5px 0px
    border-bottom 1px solid #a9aeb9
    border-top 1px solid #a9aeb9
    border-left 1px solid #a9aeb9
    border-right 1px solid #a9aeb9
    font-family Helvetica
    background-color #d4cdc8
    color #444
    text-align center
    max-width 100%
#formularios-sidebar .formulario[formulario-seleccionado=true]
    background-color #456b71
    color #ececec

#formularios-sidebar .formulario .nombre-formulario
    font-size 70%
    padding 5px 0

#floating-grid
    float left
    margin-top 5px
        
[id^=mobile-layout]
  font-size 120%
  .my-grid tr td
    vertical-align top
    white-space normal
  .nombre-informante
    font-weight bold
    font-size 110%
