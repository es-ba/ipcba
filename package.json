{
  "name": "ipcba",
  "version": "0.5.14",
  "license": "MIT",
  "description": "Indice de Precios al Consumidor",
  "main": "dist/server/server-ipcba.js",
  "files": [
    "dist",
    "install"
  ],
  "dependencies": {
    "@material-ui/core": "~4.7.1",
    "@material-ui/icons": "~4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.33",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "redux": "^4.0.4",
    "react-redux": "^7.1.3",
    "backend-plus": "~0.31.11",
    "ajax-best-promise": "^0.3.7",
    "best-globals": "^0.10.23",
    "like-ar": "^0.2.19",
    "pg-triggers": "~0.2.3",
    "redux-typed-reducer": "~0.1.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "server4test": "^0.5.3",
    "types.d.ts": "^0.4.10",
    "mixin-patch": "~0.3.1",
    "material-styles": "~0.0.6"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-tests.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\") && (tsc -p tsconfig-unlogged.json || echo \"continue w/error\") && mixin-patch",
    "watch:buildC": "(tsc -p tsconfig-client.json --watch || echo \"continue w/error\")",
    "watch:buildS": "(tsc -p tsconfig-server.json --watch || echo \"continue w/error\")",
    "watch:buildU": "(tsc -p tsconfig-unlogged.json --watch || echo \"continue w/error\")",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "dump": "npm start -- --dump-db",
    "dumpb": "(tsc -p tsconfig-server.json  || echo \"continue w/error\") && npm run dump",
    "start": "node dist/server/server-ipcba.js",
    "demo-dm": "cd demo && node ../node_modules/server4test/lib/launch4test.js && cd .."
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 90,
    "run-in": "server",
    "stability": "extending",
    "type": "app",
    "ecmaVersion": 6
  }
}
